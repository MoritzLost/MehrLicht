    "scripts": {
        "build": "npm run build:eleventy && npm run build:sass",
        "build:eleventy": "eleventy",
        "build:sass": "node-sass --output-style=compressed --omit-source-map-url src/sass --output=dist/css/",
        "start": "npm-run-all build:sass -p watch:*",
        "watch:eleventy": "eleventy --serve",
        "watch:sass": "node-sass --watch --output-style=expanded --source-maps=true src/sass --output=dist/css"
    },
    "dependencies": {
        "@11ty/eleventy": "^0.10.0",
        "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.0",
        "@fortawesome/fontawesome-free": "^5.11.2",
        "hamburgers": "^1.1.3",
        "jsdom": "^16.0.0",
        "markdown-it": "^10.0.0",
        "markdown-it-anchor": "^5.2.5",
        "node-sass": "^4.12.0",
        "prism-themes": "^1.3.0",
        "rfs": "^9.0.2"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}
